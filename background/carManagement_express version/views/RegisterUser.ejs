  <section id="contact-page">
    <div class="container">
      <div class="center">
        <h2>  </h2>
        <h2>註冊新成員</h2>

      </div>
      <div class="row contact-wrap">
        <div class="status alert alert-success" style="display: none"></div>
        <form id="main-contact-form" class="contact-form" name="contact-form" method="post" action="/index">
          <div class="col-sm-5 col-sm-offset-1">
            <div class="form-group">
              <label>權限</label>
              <select id="whoami" name="whoami" class="form-control" required="required">
                <%var options = accounts
                for ( var i = 0; i < accounts.length; i++ )
                {%>
                  <option value="<%=accounts[i]%>"><%=accounts[i] %></option>
                <%
                }
                %>
              </select>

            </div>
            <div class="form-group">
              <label>身分證字號</label>
              <input id="UserID" name="UserID" type="text" placeholder="請輸入身分證字號" class="form-control" required="required">
            </div>
            <div class="form-group">
              <label>E-mail</label>
              <input id="Email" name="Email" type="email" placeholder="請輸入信箱" name="email" class="form-control" required="required">
            </div>
            <div class="form-group">
              <label>密碼</label>
              <input id="Password" name="Password" type="password" class="form-control" placeholder="請輸入密碼">
            </div>
          </div>
          <div class="col-sm-5">
            <!-- <div class="form-group">
              <label>Subject *</label>
              <input type="text" name="subject" class="form-control" required="required">
            </div> -->
            <div class="form-group">
              <button id="userSubmit" type="submit" class="btn btn-primary btn-lg">Submit</button>
            </div>
            <div class="form-group">
              <label>活動紀錄 *</label>
              <textarea id="logger" name="message" id="message" required="required" class="form-control" rows="8"></textarea>
            </div>
          </div>

        </form>
      </div>
      <!--/.row-->
    </div>
    <!--/.container-->
  </section>
